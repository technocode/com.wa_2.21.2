<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android" android:layout_width="match_parent" android:layout_height="match_parent">
    <LinearLayout android:orientation="vertical" android:layout_width="match_parent" android:layout_height="match_parent">
        <View android:id="@+id/transition_clipper_top" android:layout_width="match_parent" android:layout_height="0dp"/>
        <androidx.recyclerview.widget.RecyclerView android:orientation="vertical" android:id="@+id/business_catalog_list" android:layout_width="match_parent" android:layout_height="match_parent"/>
        <View android:id="@+id/transition_clipper_bottom" android:layout_width="match_parent" android:layout_height="0dp"/>
    </LinearLayout>
    <FrameLayout android:id="@+id/business_catalog_host" android:layout_width="match_parent" android:layout_height="match_parent"/>
</FrameLayout>
