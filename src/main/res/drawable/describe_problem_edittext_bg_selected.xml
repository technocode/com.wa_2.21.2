<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@color/settings_contact_us_describe_bg"/>
            <corners android:topLeftRadius="6dp" android:topRightRadius="6dp" android:bottomLeftRadius="0dp" android:bottomRightRadius="0dp"/>
        </shape>
    </item>
    <item android:left="-3dp" android:top="-3dp" android:right="-3dp">
        <shape>
            <solid android:color="@android:color/transparent"/>
            <stroke android:width="2dp" android:color="@color/primary_light"/>
        </shape>
    </item>
</layer-list>
